{
  "name": "javascript-algorithms",
  "version": "1.0.0",
  "description": "",
  "main": ".eslintrc.js",
  "dependencies": {},
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.3.0",
    "eslint": "^7.7.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "jest": "^26.4.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2"
  },
  "scripts": {
    "clean:unit-tests": "rimraf ./coverage",
    "eslint": "eslint --ext .js --ignore-path .eslintignore .",
    "lint": "npm run eslint",
    "lint:fix": "npm run eslint -- --fix",
    "run:unit-tests": "jest --runInBand",
    "test": "run-s clean:unit-tests \"run:unit-tests -- {1}\" --",
    "test:pattern": "run-s clean:unit-tests \"run:unit-tests -- --t {1}\" --",
    "test:update": "run-s clean:unit-tests \"run:unit-tests -- -u {1}\" --",
    "test:watch": "run-s clean:unit-tests \"run:unit-tests -- --watch {1}\" --"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/KeionneDerousselle/javascript-algorithms.git"
  },
  "author": "",
